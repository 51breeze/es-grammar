{
    "name": "es",
    "displayName": "EaseScript",
    "description": "A EaseScript code grammar",
	"publisher":"EaseScript",
    "version": "1.0.4",
    "engines": {
        "vscode": "^1.54.0"
    },
    "icon": "images/logo.png",
    "categories": [
        "Programming Languages"
    ],
    "main":"./lib/extension",
    "activationEvents": [
        "onLanguage:easescript"
    ],
    "contributes": {
        "languages": [{
            "id": "easescript",
            "aliases": ["es","ES","easescript"],
            "extensions": [".es",".ES"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "easescript",
            "scopeName": "source.es",
            "path": "./syntaxes/easescript.tmLanguage.json"
        }]
    },
    "devDependencies": {
        "easescript2": "^0.0.1"
    },
    "keywords": [
		"EaseScript",
		"es"
	],
	"author": "664371281@qq.com"
}